{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/simple",
    "type": "object",
    "properties": {
        "timestamp": {
            "description": "Time when value is captured",
            "type": "integer"
        },
        "entityRef": {
            "description": "Description of references defining current value",
            "type": "string"
        },
        "status": {
            "description": "Status of the tracking entity",
            "type": "string"
        }
    },
    "required": ["timestamp", "name", "status"]
}
